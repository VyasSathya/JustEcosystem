{
  "name": "task-doc-management-template",
  "version": "1.0.0",
  "description": "Template for integrated task & documentation management system",
  "main": "index.js",
  "scripts": {
    "ai:status": "node scripts/ai-agent.js status",
    "ai:suggest": "node scripts/ai-agent.js suggest",
    "brand:apply": "node scripts/brandify.js",
    "dash:start": "node scripts/dashboard-server.js",
    "repo:new": "node scripts/new-repo.js",
    "list:scripts": "node scripts/list-scripts.js",
    "lint:all": "eslint .",
    "task:open": "node scripts/task-cli.js",
    "doc:open": "node scripts/doc-cli.js",
    "fix:markdown": "node scripts/fix-markdown-lint.js",
    "start": "node index.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "task": "node scripts/task-cli.js",
    "doc": "node scripts/doc-cli.js",
    "fix-markdown": "node scripts/fix-markdown-lint.js",
    "install-markdown-extensions": "node scripts/install-markdown-extensions.js",
    "postinstall": "node scripts/setup.js"
  },
  "bin": {
    "task-management": "./scripts/task-cli.js",
    "doc-management": "./scripts/doc-cli.js"
  },
  "keywords": [
    "template",
    "typescript",
    "documentation",
    "task-management"
  ],
  "author": "{AUTHOR}",
  "license": "MIT",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "express": "^5.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.2.6",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.4.3",
    "node-fetch": "^3.3.2",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
